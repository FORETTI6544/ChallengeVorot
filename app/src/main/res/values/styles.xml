<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CircularShape">
        <item name="cornerFamily">rounded</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="DialogStyle" parent="Theme.Abchihba">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:padding">5dp</item>
        <item name="cornerRadius">5dp</item>
        <item name="android:windowBackground">@drawable/design_window</item>
    </style>

    <style name="buttonStyle" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/button_default</item>
    </style>

    <style name="AutoCompleteTextViewStyle" parent="Widget.AppCompat.AutoCompleteTextView">
        <item name="android:popupBackground">@drawable/frame_primary</item>
        <item name="android:dropDownListViewStyle">@style/DividerAutoComplete</item>
    </style>

    <style name="DividerAutoComplete" parent="Widget.AppCompat.ListView.DropDown">
        <item name="android:divider">@color/white</item>
        <item name="android:dividerHeight">1dp</item>
    </style>


</resources>